{
  "expo": {
    "name": "VitalVoice",
    "slug": "VitalVoice",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/VitalVoiceLogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/VitalVoiceLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#DBE9EE"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/VitalVoiceLogo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": ""
    },
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "expo-build-properties"
    ]
  }
}
